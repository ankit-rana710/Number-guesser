#to import random module
import random
#to create a range of random numbers between 1-10
n=random.randrange(1,100)
guess=int(input("Enter any number:"))
while n!=guess:
    if guess<n:
        print("too low")
        guess=int(input("Enter number again: "))
    elif guess>n:
        print("Too high")
        guess=int(input("Enter number again:"))
    else:
        break
